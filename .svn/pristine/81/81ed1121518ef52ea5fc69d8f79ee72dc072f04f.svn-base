    public void displayReservedRooms(){
        
        guest = database.getGuest();
        System.out.println("Guest name: "+guest.getName());
        
        firstNameLabel.setText(guest.getfName());
        lastNameLabel.setText(guest.getName());
        midNameLabel.setText(guest.getMName());
        birthdateLabel.setText(guest.getBday());
        mobileLabel.setText(guest.getMobileNum());
        emailLabel.setText(guest.getEmail());
        
        getMyRooms();
        
        System.out.println("roomCounter: "+roomCounter);
        System.out.println("room1: "+Integer.toString(roomNums[0]));
        System.out.println("room2: "+Integer.toString(roomNums[1]));
        System.out.println("room3: "+Integer.toString(roomNums[2]));
        System.out.println("room4: "+Integer.toString(roomNums[3]));
        System.out.println("room5: "+Integer.toString(roomNums[4]));
        
        rooomNum1.setText(Integer.toString(roomNums[0])); 
        rooomNum2.setText(Integer.toString(roomNums[1])); 
        rooomNum3.setText(Integer.toString(roomNums[2])); 
        rooomNum4.setText(Integer.toString(roomNums[3])); 
        rooomNum5.setText(Integer.toString(roomNums[4])); 
                
        roomType1.setText(database.getSelectedRoomType() + " bed");
        roomType2.setText(database.getSelectedRoomType() + " bed");
        roomType3.setText(database.getSelectedRoomType() + " bed");
        roomType4.setText(database.getSelectedRoomType() + " bed");
        roomType5.setText(database.getSelectedRoomType() + " bed");
        
    }
    
    public void getMyRooms(){
        myRooms = database.getGuestRooms();
        roomCounter = 0;
        
        for(int i=0;i<database.getRoomCounter();i++){
            if(guest.getAccountID() == myRooms[i].getOccupantID() ){
                roomNums[roomCounter] = myRooms[i].getRoomNum(); //get the room number of guest
                roomCounter++;
            }
        }
                
    }

