    /**
     * When save button is clicked, It will verify if the guest really want to cancel
     * the room reservations and ask for the guest account ID. If the ID is valid 
     * it will cancel all selected room reservation. 
     */
    private void saveButtonActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_saveButtonActionPerformed
        int status=0;
        int guestID;
       // try{
        //show dialog box for confirmation
            status = JOptionPane.showConfirmDialog(jPanel2, "Do you really want to cancel reservations?", 
                    "Save changes", JOptionPane.YES_NO_OPTION);
        
            System.out.println("status: " + status);
        
        if(status == 0){
            
            guestID = parseInt(JOptionPane.showInputDialog(jPanel1, "Enter your account ID:"));
            
            if(database.verifyGuest(guestID)){  
                
                try {
                    //updates the changes in reservation in the database
                    database.updateRoomChanges_DB();
                } catch (SQLException ex) {
                    Logger.getLogger(ManageAccountsFrame1.class.getName()).log(Level.SEVERE, null, ex);
                }

                GuestOptionFrame option = new GuestOptionFrame();
                option.dispFrame();
                this.setVisible(false);

                JOptionPane.showMessageDialog(jPanel1, "You have successfully cancelled your reservations. ");

                System.out.println("Save changes in reservation.");
                roomCanceledCount= 0;
            }else{
                JOptionPane.showMessageDialog(jPanel1, "Invalid Account ID. ");
            }
        }else{
            
            System.out.println("canceled changes in reservation.");
        }
        //}catch(Exception e){ 
        //    JOptionPane.showMessageDialog(jPanel1, "Error saving changes. Please contact "
        //            + "the technical support to solve this problem. ");
       // }
        
    }//GEN-LAST:event_saveButtonActionPerformed

