    /**
     * When these buttons have been pressed or selected, increase the canceled room
     * count and reset the room status to be free to be reserved. if this is deselected,
     * set again the status of the guest to this room including its reservation dates.
     */
    private void cancelButtonActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_cancelButtonActionPerformed

        if(cancelButton.isSelected()){
            roomCanceledCount++;    // increase canceled room count
            rooomNum1.setForeground(Color.red);
            roomType1.setForeground(Color.red);
            startDate1.setForeground(Color.red);
            endDate1.setForeground(Color.red);
            
            roomNums[0].setAvailability(true);
            roomNums[0].setOccupant("none");
            roomNums[0].setOccupantID(0);
            
            //transfer dates to temporary Storage for later retrieval
            tempStartDate1 = roomNums[0].getStartDate(); 
            tempEndDate1 = roomNums[0].getEndDate(); 
            roomNums[0].setStartDate("none");
            roomNums[0].setEndDate("none");
            
            System.out.println("Reservation @ room " + roomNums[0].getRoomNum());
            System.out.println("myRooms[0]: " + roomNums[0].getStatus());
            System.out.println("getName: " + roomNums[0].getOccupant());
            System.out.println("getAccountID: " + roomNums[0].getOccupantID());
            System.out.println("tempStartDate: " + roomNums[0].getStartDate());
            System.out.println("tempEndDate: " + roomNums[0].getEndDate());
            
            System.out.println("cancel reservation @ room " + roomNums[0].getRoomNum());
        }else if(!cancelButton.isSelected()){ // deselect
            roomCanceledCount--;    // decrease canceled room count
            rooomNum1.setForeground(Color.black);
            roomType1.setForeground(Color.black);
            startDate1.setForeground(Color.black);
            endDate1.setForeground(Color.black);
            
            roomNums[0].setAvailability(false);
            roomNums[0].setOccupant(guest.getName());
            roomNums[0].setOccupantID(guest.getAccountID());
            roomNums[0].setStartDate(tempStartDate1);
            roomNums[0].setEndDate(tempEndDate1);
            
            System.out.println("Reservation @ room " + roomNums[0].getRoomNum());
            System.out.println("myRooms[0]: " + roomNums[0].getStatus());
            System.out.println("getName: " + guest.getName());
            System.out.println("getAccountID: " + guest.getAccountID());
            System.out.println("tempStartDate: " + tempStartDate1);
            System.out.println("tempEndDate: " + tempEndDate1);
        }
        
        updateSaveButton();
    }//GEN-LAST:event_cancelButtonActionPerformed

